############ Setting items of All servers ############
[all:vars]
## Hostname
# EX: {Tag_Name}->1-server.example
tag_Name=personium.example.com

## Master Token of Personium
# EX: {Master_Token}->abc123
master_token=example_master_token

## Web server FQDN
# EX: {Personium_FQDN}->1-server.example
base_url=personium.example.com

## URL format to access cell
## true:path based cell url
## false:per cell fqdn url
# EX: {Path_Based_Cell_Url_Enabled}->false
path_based_cell_url_enabled=true
